"Syntax highlighting for config files
autocmd BufWritePre,BufNewFile,BufRead ~/.vim_my_settings/* set syntax=vim
autocmd BufWritePre,BufNewFile,BufRead ~/.vim_my_settings/* set filetype=vim
autocmd BufWritePre,BufNewFile,BufRead ~/.vim/ftplugin/* set syntax=vim
autocmd BufWritePre,BufNewFile,BufRead ~/.vim/ftplugin/* set filetype=vim
autocmd BufWritePre,BufNewFile,BufRead ~/.config_file_list set filetype=vim
autocmd BufWritePre,BufNewFile,BufRead ~/.zsh_my_settings/* set syntax=zsh
autocmd BufWritePre,BufNewFile,BufRead ~/.zsh_my_settings/* set filetype=zsh
autocmd BufWritePre,BufNewFile,BufRead ~/.zshrc set filetype=zsh
autocmd BufWritePre,BufNewFile,BufRead ~/.vim/Snippets/*Javascript set syntax=javascript
autocmd BufWritePre,BufNewFile,BufRead ~/.vim/Snippets/*Javascript set filetype=javascript


